SELECT * FROM STUDENTS;

 DESC STUDENTS;

 SELECT ROLL_NO ,NAME,CLASS FROM STUDENTS WHERE CITY='AGRA';
 SELECT * FROM STUDENTS WHERE MARKS>50;
 SELECT * FROM STUDENTS ORDER BY MARKS DESC;
 SELECT * FROM STUDENTS ORDER BY MARKS ASC;
 UPDATE STUDENTS SET NAME='ARYAN' ,MARKS='92' WHERE ROLL_NO='1';
 SELECT * FROM STUDENTS;
 UPDATE STUDENTS SET MARKS='89' WHERE ROLL_NO='3';
 SELECT * FROM STUDENTS;

 DELETE FROM STUDENTS WHERE ROLL_NO='4';
SELECT * FROM STUDENTS;

DELETE FROM STUDENTS WHERE MARKS<=45;
SELECT * FROM STUDENTS;
