CREATE TABLE STUDENTS(
    ROLL_NO  INT,
    NAME VARCHAR(60),
    dob DATE ,
    GENDER CHAR,
    CLASS INT,
    COLLEGE VARCHAR(60),
    CITY VARCHAR(60),
    MARKS INT ,
    PRIMARY KEY(ROLL_NO)
);
